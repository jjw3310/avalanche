{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@data/*": ["./src/data/*"],
      "@config/*": ["./src/config/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@mock/*": ["./src/@mock/*"],
      "@contexts/*": ["./src/@contexts/*"],
      "@styles/*": ["./src/@styles/*"],
    }
  },
  "exclude": ["node_modules", "build"]
}